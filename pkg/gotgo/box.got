package box(type ta, type tb ta, type tk string)

// Here we have some utility routines for handling "boxing"
// conversions from a to b, where a is assignment-compatible with b.

func Box(as []ta) []tb {
	bs := make([]tb, len(as))
	for i,a := range as {
		bs[i] = a
	}
	return bs
}

func BoxMap(ma map[tk]ta) (mb map[tk]tb) {
	for i,a := range ma {
		mb[i] = a
	}
	return
}
